{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**/*.@(js|css)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "https://sccopy-38403.bubbleapps.io"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/generateTokens",
        "function": "generateTokens"
      }
    ]
  }
}
