{
  "name": "startupcorners-agora-client",
  "version": "1.0.0",
  "description": "Client library for Startup Corners Video Chat Implementation using Agora",
  "main": "index.js",
  "scripts": {
    "serve": "node index.js",
    "build": "esbuild ./public/js/main.js --bundle --minify --splitting --format=esm --outdir=public/dist",
    "dev": "esbuild ./public/js/main.js --bundle --minify --splitting --format=esm --outdir=public/dist --watch",
    "dev-style": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch"
  },
  "author": "andhikayuana",
  "license": "ISC",
  "devDependencies": {
    "esbuild": "0.20.2",
    "express": "4.19.2",
    "tailwindcss": "^3.4.3"
  },
  "dependencies": {
    "agora-extension-virtual-background": "^1.3.0",
    "agora-rtc-sdk-ng": "4.20.2",
    "agora-rtm-sdk": "^2.2.0",
    "axios": "^1.7.9"
  }
}
