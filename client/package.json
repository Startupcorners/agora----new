{
  "name": "startupcorners-agora-client",
  "version": "1.0.0",
  "description": "Client library for Startup Corners Video Chat Implementation using Agora",
  "main": "index.js",
  "scripts": {
    "serve": "node index.js",
    "build": "esbuild ./src/main.js --bundle --minify --splitting --format=esm --outdir=dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "esbuild ./src/main.js --bundle --minify --splitting --format=esm --outdir=dist --watch",
    "dev-style": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch"
  },
  "author": "andhikayuana",
  "license": "ISC",
  "devDependencies": {
    "esbuild": "0.20.2",
    "express": "4.19.2",
    "tailwindcss": "^3.4.3"
  },
  "dependencies": {
    "agora-rtc-sdk-ng": "4.20.2"
  }
}
